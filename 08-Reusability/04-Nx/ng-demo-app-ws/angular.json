        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": "143f16a4-c725-4146-97ca-fa070d04bc8e"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
